{
  "name": "example/wordpress",
  "description": "A sample WordPress project",
  "type": "project",
  "require": {
    "php": "^7.4 || ^8.0",
    "wp-cli/wp-cli": "^2.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
